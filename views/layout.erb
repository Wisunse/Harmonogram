<!DOCTYPE html>

<html ng-app="Harmonogram" ng-controller="MainController" ng-init="init()">

  <head>

    <title> Harmonogram </title>

    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport">

    <link rel="stylesheet" property="stylesheet" href="stylesheet/libs/angular-material.min.css">
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
    <link rel="stylesheet" property="stylesheet" href="stylesheet/css/app.css">

    <link rel="shortcut icon" href="img/icon3.png">

  </head>

  <body>

    <%= yield %>

  </body>

    <script src="scripts/libs/angular.min.js"></script>
    <script src="scripts/libs/angular-ui-router.min.js"></script>
    <script src="scripts/libs/angular-sanitize.min.js"></script>

    <script src="scripts/libs/angular-animate.min.js"></script>
    <script src="scripts/libs/angular-aria.min.js"></script>
    <script src="scripts/libs/angular-messages.min.js"></script>
    <script src="scripts/libs/angular-material.min.js"></script>

  <% if $deployment == 'production' %>

    <script src="scripts/application.min.js"></script>

  <% else %>

    <script src="scripts/dev/app.js"></script>

    <script src="scripts/dev/directives/app.directive.js"></script>

    <script src="scripts/dev/services/app.service.js"></script>
    <script src="scripts/dev/services/users.service.js"></script>
    <script src="scripts/dev/services/cars.service.js"></script>
    <script src="scripts/dev/services/management.service.js"></script>

    <script src="scripts/dev/controllers/main.controller.js"></script>
    <script src="scripts/dev/controllers/login.controller.js"></script>
    <script src="scripts/dev/controllers/management.controller.js"></script>
    <script src="scripts/dev/controllers/menu.controller.js"></script>
    <script src="scripts/dev/controllers/users.controller.js"></script>
    <script src="scripts/dev/controllers/cars.controller.js"></script>
    <script src="scripts/dev/controllers/dialog.controller.js"></script>

  <% end %>



</html>